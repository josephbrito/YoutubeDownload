{
    "builds": [{
        "src": "myapp/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "myapp/wsgi.py",
        "use": "@vercel/static-build",
        "config": { "distDir": "staticfiles_build" }
    }
],
    "routes": [
        {
            "src": "myapp/youtube/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "myapp/wsgi.py"
        }
    ]
}